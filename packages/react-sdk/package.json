{
  "name": "@payroo-group/embed-sdk-react",
  "version": "0.2.6",
  "description": "Payroo embed components for React applications",
  "type": "module",
  "license": "MIT",
  "homepage": "https://github.com/payroo-group/embed-sdk#readme",
  "bugs": {
    "url": "https://github.com/payroo-group/embed-sdk/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/payroo-group/embed-sdk.git"
  },
  "author": "Shidil Eringa <shidil@payroo.com.au>",
  "files": [
    "dist"
  ],
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.cts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./package.json": "./package.json"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "lint": "eslint --cache .",
    "lint:fix": "npm run lint --fix",
    "build": "tsdown",
    "dev": "tsdown --watch",
    "playground": "vite --config playground/vite.config.ts",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "@payroo-group/embed-sdk": "^0.2.5"
  },
  "peerDependencies": {
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.30.1",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@types/node": "^22.15.17",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@vitejs/plugin-react": "^4.6.0",
    "eslint": "^9.30.1",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "happy-dom": "^17.4.6",
    "prettier": "^3.5.3",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "tsdown": "^0.12.9",
    "tsx": "^4.19.4",
    "typescript": "^5",
    "vite": "npm:rolldown-vite@latest",
    "vitest": "^3.1.3"
  }
}
